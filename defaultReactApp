Component형
import React, { Component } from "react";
import PropTypes from "prop-types";

class App extends Component {
  constructor(props) {
    super(props);
  }

  render() {
    return (
      <div>
        <h1>App</h1>
      </div>
    );
  }
}

App.propTypes = {};
App.defaultProps = {};

export default App;





함수형
import React from "react";

const App = () => {
  return (
      <div>
        <h1>App</h1>
      </div>
  );
}

export default App;
